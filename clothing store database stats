CREATE TABLE clothing_store (id INTEGER PRIMARY KEY,clothing TEXT,sold_items INTEGER,price INTEGER);
INSERT INTO clothing_store VALUES ( 1, "jeans",20,60);
INSERT INTO clothing_store VALUES (2,"shirts",25,25);
